    tl = 0;
    ts = "";
    td = "";
}

tx1 = view_wport / 2; //draw from the middle of the screen
tx2 = view_wport / 4 * 3; //max width of 3/4 of the screen
ty = view_hport - font_get_size(tf);
</string>
