            <string>if !ds_queue_empty(tq)
{
    ts = ds_queue_head(tq) //put the upmost value of the text queue into storage
    tl = string_length(ts) //get the length of the tring
    
    if tc &lt; tl //dont let the letter count go over the string length
    {
        tc += tr //increase the letter count
    }
    else if alarm[0] == -1
    {
        alarm[0] = 15 + sqrt(tl * 3); //set the alarm after the string is fully copied
    }
    
    td = string_copy(ts, 0, tc) //put the counted value of ts into td
}
else
